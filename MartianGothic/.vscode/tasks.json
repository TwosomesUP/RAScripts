{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build current rascript",
      "type": "shell",
      "command": "${config:rascript.cliPath}",
      "args": [
        "-v",
        "-i", "${file}",
        "-o", "${config:rascript.outputPath}",
        "-a", "BacklogOddy"
      ],
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}